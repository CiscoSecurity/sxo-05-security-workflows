        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Create filter JSON",
          "script": "import sys,json\nfilterObject = {\n  \"name\": sys.argv[1],\n  \"entries\": [\n    {\n      \"url\": sys.argv[2],\n      \"type\": \"simple\",\n      \"action\": \"block\",\n      \"status\": \"enable\"\n    }\n  ]\n}\nfilterObject = json.dumps(filterObject)",
