			"action_timeout": 180,
			"continue_on_failure": true,
			"display_name": "Prepare updated pre rule payload",
			"script": "import json\nfrom sys import argv\nfrom collections import namedtuple\n# --------------------------------------------------------------\nObj = namedtuple('Obj', 'field singles value')\nclass Object(Obj):\n    def _clean_from_singles(self, values: list):\n        if len(values) == 1 and (values[0] in self.singles):\n            values.clear()\n    def _insert_if_not_contain(self, container: list):\n        if self.value not in container:\n            container.append(self.value)\n    def inject_to(self, body: dict):\n        if body.get(self.field):\n            self._clean_from_singles(body[self.field]['member'])\n            self._insert_if_not_contain(body[self.field]['member'])\n        else:\n            body.update({self.field: {'member': [self.value]}})\n            \n            \ndef remove_extra_fields(extra_fields, body):\n    for field in extra_fields:\n        body.pop(field, None)\n    return body\n# --------------------------------------------------------------\nextra_fields = ('@uuid', '@location', '@device-group')\n# fields_to_update contain field name and tuple of values,\n# which have to be removed if we want to set object instead\nfields_to_update = (\n    ('source', ('any',)),\n    ('destination', ('any',)),\n    ('category', ('any',)),\n    ('service', ('application-default', 'any'))\n)\nrule_body = remove_extra_fields(extra_fields, json.loads(argv[1]))\nfor field_singles, value in zip(fields_to_update, argv[3:7]):\n    if value:\n        Object(*field_singles, value).inject_to(rule_body)\naction = argv[2]\nif action:\n    rule_body['action'] = action.lower()\npayload = json.dumps({'entry': rule_body})",
