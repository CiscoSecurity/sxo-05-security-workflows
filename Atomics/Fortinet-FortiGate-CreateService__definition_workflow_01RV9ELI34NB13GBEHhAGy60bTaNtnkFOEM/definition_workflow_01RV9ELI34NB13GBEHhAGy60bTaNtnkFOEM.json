		  "properties": {
			"action_timeout": 180,
			"continue_on_failure": false,
			"display_name": "Create service JSON",
			"script": "from sys import argv\nimport json\ndestination_port = argv[1].strip()\nsource_port = argv[2].strip()\nservice_name = argv[3].strip()\nprotocol = argv[4].strip()\ncategory = argv[5].strip()\n\nservice_json = {\n        \"name\": service_name,\n        \"protocol\": protocol,\n        \"category\": category,\n        f\"{protocol}-portrange\": f\"{destination_port}:{source_port}\"\n}\nservice_json = json.dumps(service_json)",
