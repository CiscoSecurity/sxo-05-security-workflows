			"action_timeout": 180,
			"continue_on_failure": false,
			"display_name": "Generate ticket JSON",
			"script": "import sys, json\nincident_params = {\n\t\"serviceDeskId\": sys.argv[1],\n\t\"requestTypeId\": sys.argv[2],\n\t\"requestFieldValues\": {\n\t\t\"summary\": sys.argv[3],\n\t\t\"description\": sys.argv[4]\n\t}\n}\nif sys.argv[5]:\n\tincident_params[\"requestFieldValues\"].update({\"customfield_10031\": {\"value\": sys.argv[5]}})\nif sys.argv[6]:\n\tincident_params[\"requestFieldValues\"].update({\"customfield_10004\": {\"value\": sys.argv[6]}})\nif sys.argv[7]:\n\tincident_params[\"requestFieldValues\"].update({\"customfield_10005\": {\"value\": sys.argv[7]}})\nif sys.argv[8]:\n\tincident_params[\"requestFieldValues\"].update({\"customfield_10006\": {\"value\": sys.argv[8]}})\nif sys.argv[9]:\n\tincident_params[\"requestFieldValues\"].update({\"customfield_10007\": {\"value\": sys.argv[9]}})\nincident_json = json.dumps(incident_params)",
