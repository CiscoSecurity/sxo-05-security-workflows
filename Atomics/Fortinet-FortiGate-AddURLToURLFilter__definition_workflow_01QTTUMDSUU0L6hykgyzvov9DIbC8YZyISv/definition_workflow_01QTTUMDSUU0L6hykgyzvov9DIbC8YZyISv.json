      {
        "unique_name": "definition_activity_01QTTUO94Y81I6sxeFCdV2MxLPZ80LZVcP5",
        "name": "Execute Python Script",
        "title": "Update URL filter JSON",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Update URL filter JSON",
          "script": "from sys import argv\nimport json\nfrom urllib.parse import urlparse\nurlfilter_json = json.loads(argv[1])[\"results\"][0]\nurl = argv[2]\nentry_template = {\n    \"type\": \"simple\",\n    \"action\": \"block\",\n    \"status\": \"enable\"\n}\nmessage = \"\"\ncorrect_entry_found_in_urlfilter = False\nurl_found_in_urlfilter = False\ndef strip_scheme(url):\n    parsed = urlparse(url)\n    scheme = f\"{parsed.scheme}://\"\n    return parsed.geturl().replace(scheme, '', 1)\nfor entry in urlfilter_json[\"entries\"]:\n    if entry[\"url\"] == strip_scheme(url):\n        if entry_template.items() < entry.items():\n            correct_entry_found_in_urlfilter = True\n            message = f\"The URL '{url}' is already in URL Filter.\"\n        else:\n            message = (\n                f\"The URL '{url}' is already in URL Filter, \"\n                \"but it has different attributes.\"\n            )\n        url_found_in_urlfilter = True\n        break\nif not url_found_in_urlfilter:\n    urlfilter_json[\"entries\"].append(\n        {\n            \"url\": url,\n            **entry_template\n        }\n    )\n    urlfilter_json = json.dumps(urlfilter_json)\nelse:\n    urlfilter_json = {}",
          "script_arguments": [
            "$activity.definition_activity_01QTTUNRADMYG2ZW8K8aJmAz7bZo4JIjUcL.output.response_body$",
