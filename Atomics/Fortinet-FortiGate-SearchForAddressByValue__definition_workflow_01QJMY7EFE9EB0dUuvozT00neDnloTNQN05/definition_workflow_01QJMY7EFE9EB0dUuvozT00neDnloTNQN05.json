        "title": "Generate query parameter",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Generate query parameter",
          "script": "from sys import argv\nobservable_value = argv[1]\nobservable_type = argv[2]\nmapping = {\n    \"domain\": \"fqdn\",\n    \"ip\": \"subnet\"\n}\nif observable_type == \"ip\":\n    observable_value = f\"{observable_value} 255.255.255.255\"\nquery_parameter = f\"{mapping[observable_type]}=={observable_value}\"",
          "script_arguments": [
