	  "actions": [
		{
		  "unique_name": "definition_activity_01RZNYWB1HMQK0uKJS6lF3V2rqadtdCK1SY",
		  "name": "Execute Python Script",
		  "title": "Create service group JSON",
		  "type": "python3.script",
		  "base_type": "activity",
		  "properties": {
			"action_timeout": 180,
			"continue_on_failure": false,
			"display_name": "Create service group JSON",
			"script": "import json\nfrom sys import argv\n\n\nservice_group_name = argv[1].strip()\nmembers = argv[2].split(',')\nproxy = json.loads(argv[3])\n\nservice_group_json = {\n        \"name\": service_group_name,\n        \"proxy\": \"enable\" if proxy else \"disable\",\n        \"member\": [{\"name\": member.strip()} for member in members],\n}\nservice_group_json = json.dumps(service_group_json)",
			"script_arguments": [
			  "$workflow.definition_workflow_01RZNYVXUXIG552KulMfFGCIBl8mRN1ZZvf.input.variable_workflow_01RZNYVY3IKCI7hk0461ZsisyRTizEggTPq$",
			  "$workflow.definition_workflow_01RZNYVXUXIG552KulMfFGCIBl8mRN1ZZvf.input.variable_workflow_01RZOD44514BX0kGUceiPoUWcHXcPoTc9Ap$",
			  "$workflow.definition_workflow_01RZNYVXUXIG552KulMfFGCIBl8mRN1ZZvf.input.variable_workflow_01S5GA6PCT5AR6HtJNJc5kLJ2lhEfOu9Axu$"
			],
			"script_queries": [
			  {
				"script_query": "service_group_json",
				"script_query_name": "JSON",
				"script_query_type": "string"
			  }
			],
			"skip_execution": false
		  },
